    "eslint-plugin-mocha": "^5.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "inflection": "^1.12.0",
    "lodash": ">=4.17.11",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "prettier": "^1.15.3",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
